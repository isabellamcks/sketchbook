def setup():
    size(550,500,P3D);
    background(0);
    lights();
    colorMode(HSB)

def draw():
    noFill()
    strokeWeight(2)
    stroke(255)
    translate(width/2, height /2)
    box(400,150,100)
    
    fill(255)
    strokeWeight(2)
    stroke(255)
    box(350,100,100)
    
    
    



    
    
